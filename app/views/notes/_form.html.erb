<%=bootstrap_form_for(@note)  do |f| %>
  <%=f.hidden_field :user_id, value: current_user.id%>
  <%=f.text_field :title, label: '제목' %><br>
  <%= f.text_area :content, label: '내용', :class => "tinymce", :rows => 20, :cols => 20 %><br>
  <%=f.submit '작성' %><br>
  <%= tinymce %>
<% end %>
<style>
  .control-label{
    font-size:20px;
  }
</style>
